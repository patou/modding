<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='moddingShowcase'>
 <inherits name="com.google.gwt.core.Core" />
 <inherits name="com.google.gwt.user.User" />
 <inherits name="com.sfeir.modding.Modding" />
 
 <!-- Specify the app entry point class.                         -->
 <entry-point class='com.sfeir.modding.client.showcase.ShowCase' />

 <generate-with class="com.sfeir.modding.generator.ShowcaseGenerator">
  <when-type-assignable
   class="com.sfeir.modding.client.showcase.ShowCase.GeneratorInfo" />
 </generate-with>
</module>